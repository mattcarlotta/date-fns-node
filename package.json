{
  "name": "date-fns-node",
  "version": "0.0.1",
  "description": "An example of using date-fns with express.",
  "repository": "https://github.com/mattcarlotta/date-fns-node",
  "author": "Matt Carlotta",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "build": "rimraf build && tsc --p ./tsconfig.prod.json",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./ --ext .ts",
    "start": "ENV_DEBUG=true ENV_LOAD=.env.prod node -r ./prod-paths.js -r snackables build/index.js",
    "dev": "nodemon --config ./nodemon.json",
    "tsc": "tsc --p ./tsconfig.json"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.21.3",
    "date-fns-tz": "^1.1.4",
    "ejs": "^3.1.6",
    "express": "^4.16.4",
    "morgan": "^1.10.0",
    "snackables": "^2.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
